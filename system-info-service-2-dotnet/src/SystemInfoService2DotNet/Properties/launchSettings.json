{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "SystemInfoService2DotNet": {
      "commandName": "Project",
      "launchBrowser": true,
      "applicationUrl": "http://localhost:80/openapi/index.html",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}",
      "publishAllPorts": true,
      "useSSL": true
    }
  }
}
